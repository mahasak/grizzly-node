{
  "name": "grizzly-node",
  "version": "1.0.0",
  "description": "Project Setup ``` npm install jest @types/jest ts-jest --save-dev ```",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "build": "tsc -p .",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahasak/grizzly-node.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mahasak/grizzly-node/issues"
  },
  "homepage": "https://github.com/mahasak/grizzly-node#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/source-map-support": "^0.5.0",
    "acorn": "^7.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.0.0",
    "jest": "^24.9.0",
    "jest-coverage-badges": "^1.1.2",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "serve": "^11.3.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm test",
      "pre-push": "npm run build && npm test"
    }
  },
  "dependencies": {}
}
